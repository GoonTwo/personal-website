  "compilerOptions": {
    "baseUrl": "./src/",
    "outDir": "./dist",
    "sourceMap": true,
    "allowSyntheticDefaultImports": true,
    "noUnusedLocals": true,
    "noImplicitAny": true,
    "module": "esNext",
    "moduleResolution": "node",
    "target": "es6",
    "jsx": "react",
    "allowJs": false
  }
}
